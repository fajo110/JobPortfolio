package test;
import org.junit.Test;
import static org.junit.Assert.*;
import org.junit.Before;
import Methods.*;
public class testAuthorName {
  
  @Test
  /**
   * @throws Exception
   *checks whether if the name is the same as it extracts it
   */
  public void testExtractAuthorsName() throws Exception {
    String test = AuthorsName.extractAuthorsName("Ola Spjuth");
    assertEquals("Ola Spjuth", test);
  }
  public void testExtractAuthorsName2() throws Exception {
    String test = AuthorsName.extractAuthorsName("Yan Xu");
    assertEquals("Yan Xu", test);
  }
}
